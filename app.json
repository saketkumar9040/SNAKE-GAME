{
  "expo": {
    "name": "SNAKE-GAME",
    "slug": "SNAKE-GAME",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#365314"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
