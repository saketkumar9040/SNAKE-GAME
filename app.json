    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#365314"
      }
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#365314"
    },
    "androidNavigationBar": {
      "backgroundColor": "#365314",
      "barStyle": "light-content"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
